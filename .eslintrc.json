{
    "env": {
        "browser": true,
        "es6": true
    },

    "extends": "standard",

    "rules": {
        "quotes": ["error", "double"],
        "indent": ["error", "tab"],
        "semi": ["error", "always"],
        "no-trailing-spaces": ["error",{"skipBlankLines":true}],
        "padded-blocks": ["error", "always"],
        "quote-props": ["error", "always"]
    }
    
}